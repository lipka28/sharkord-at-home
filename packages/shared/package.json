{
  "name": "@sharkord/shared",
  "version": "0.0.4",
  "type": "module",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "format": "prettier --write \"**/*.ts*\" --config ./.prettierrc.json",
    "lint": "eslint ./src/**/*.ts --ext ts --report-unused-disable-directives",
    "lint:fix": "eslint ./src/**/*.ts --ext ts --report-unused-disable-directives --fix",
    "check-types": "tsc --noEmit",
    "magic": "bun --bun run lint:fix && bun --bun run format && bun --bun run check-types"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5",
    "@types/linkify-it": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "eslint": "^8.56.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.6",
    "js-sha256": "^0.11.1",
    "linkify-it": "^5.0.0",
    "mediasoup": "^3.19.9",
    "zod": "^4.3.5"
  }
}
